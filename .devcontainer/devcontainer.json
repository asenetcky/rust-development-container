{
  "name": "Rust Development",
  "image": "mcr.microsoft.com/devcontainers/rust:latest",
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer"
      ],
      "settings": {
        "editor.lineNumbers": "relative"
      }
    }
  },
  "postCreateCommand": "cargo init",
  "remoteUser": "vscode"
}
